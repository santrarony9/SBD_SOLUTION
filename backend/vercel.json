{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "prisma/**",
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/main.ts"
        },
        {
            "src": "/(.*)",
            "dest": "src/main.ts"
        }
    ]
}